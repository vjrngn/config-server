{
  "name": "config-server",
  "version": "0.0.0",
  "private": false,
  "maintainers": ["Vijay Rangan"],
  "scripts": {
    "lint": "eslint . --ext .ts",
    "start": "tsc && node dist/server.js",
    "start:dev": "nodemon src/server.ts",
    "test": "mocha",
    "test:only": "mocha -g",
    "migration:create": "typeorm migration:create --name",
    "migration:run": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:revert",
    "make": "hygen"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "lodash.isempty": "^4.4.0",
    "moment": "^2.29.1",
    "morgan": "~1.9.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "twig": "~0.10.3",
    "typeorm": "^0.2.34",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/lodash.isempty": "^4.4.6",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.41",
    "@types/sinon": "^10.0.0",
    "@types/uuid": "^8.3.1",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "chai": "^4.3.4",
    "eslint": "^7.30.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "expect": "^27.1.0",
    "hygen": "^6.1.0",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7",
    "sinon": "^10.0.0",
    "testcontainers": "^7.12.1",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
